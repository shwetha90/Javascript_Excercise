var arr=[10,20,30,40];//declaring array

var a=prompt("Enter number of elements you want to disply","enter the number");//taking input from the user

if(a<=0 || a>arr.length)//checking for the array index

{
    
alert("wrong input or you input is out of range");
  
}
 
    
else if(true)//used to print the output, if the input is within the bound
    
{
      
for(i=a;i>=0;i--)// this loop is used to print specified number of elements
        
{
     
alert("the array elements are"+ arr[i]); 
       
 }
   
 }
